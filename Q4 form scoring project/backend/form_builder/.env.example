# Form Builder API - Environment Variables
# Copy this file to .env and fill in the values

# Google Cloud Project
PROJECT_ID=opex-data-lake-k23k4y98m

# BigQuery Dataset
DATASET_ID=form_builder

# JWT Secret (same as auth API)
JWT_SECRET_KEY=your-jwt-secret-key-here

# GitHub Configuration (for form deployment)
GITHUB_TOKEN=your-github-personal-access-token
GITHUB_REPO_OWNER=opextech
GITHUB_REPO_NAME=forms
GITHUB_BRANCH=main

# Form Webhook URL (existing endpoint for form submissions)
FORM_WEBHOOK_URL=https://opex-form-webhook-4jypryamoq-uc.a.run.app

# Optional: Cloud Run Configuration
PORT=8080
