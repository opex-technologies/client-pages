.pytest_cache/
.coverage
htmlcov/
*.log

# Virtual environments
venv/
env/
ENV/

# IDE
.vscode/
.idea/
*.swp

# Documentation
README.md
*.md

# Tests
tests/
test_*.py

# Local development
.env
.env.local

# Git
.git
.gitignore
